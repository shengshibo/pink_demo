.templateContainer {
  display: flex;
  flex-direction: column;
  border-top: 1rpx solid #EEE;
  border-bottom: 1rpx solid #EEE;
  margin: 10rpx 0rpx;
}
.avatar {
  padding: 10rpx;
}
.avatar_date image {
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.avatar_date text {
  font-size: 32rpx;
}
.company {
  font-size: 36rpx;
  font-weight: 700;
  margin: 10rpx;
}
.contentImg {
  width: 100%;
  height: 460rpx;
}
.content {
  font-size: 32rpx;
  text-indent: 32rpx;
}
.collection_love image {
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.collection_love text {
  font-size: 28rpx;
  margin-right: 10rpx;
}